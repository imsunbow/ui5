<mvc:View controllerName="uiux.test2.uiuxtest2.controller.RightView"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:c="sap.suite.ui.microchart">
    <Page id="RightPage" title="{i18n>title}">
        <HBox>
            <Button text= "메인으로 이동" press= ".goMain"/>
            <Button text="BaseBall" press=".goLeft"/>
        </HBox>
        
        <FlexBox width="15rem" height="15rem" alignItems="Center" justifyContent="Center" class="sapUiSmallMargin">
            <c:InteractiveLineChart points = "{ path: 'data>/gt_data' }" displayedPoints="8"> 
                <c:points>
                    <c:InteractiveLineChartPoint value="{=parseFloat(${data>Val})}" label="{data>Label}"/>
                </c:points>
            </c:InteractiveLineChart>
        </FlexBox>

    </Page>
</mvc:View>
